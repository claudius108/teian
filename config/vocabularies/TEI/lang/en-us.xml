    <teian:annotator id="teiannHeadBtn">
        <teian:toolbar-button-title>Insert a head for list</teian:toolbar-button-title>
        <teian:toolbar-button-label/>
    </teian:annotator>
    <teian:annotator id="teiannItemBtn">
        <teian:toolbar-button-title>Insert an item for list</teian:toolbar-button-title>
        <teian:toolbar-button-label/>
    </teian:annotator>
    <teian:annotator id="teiannTableBtn">
        <teian:toolbar-button-title>Insert a table</teian:toolbar-button-title>
        <teian:toolbar-button-label/>
    </teian:annotator>
    <teian:annotator id="teiannRowBtn">
        <teian:toolbar-button-title>Insert a row for table</teian:toolbar-button-title>
        <teian:toolbar-button-label/>
