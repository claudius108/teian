        <teian:common-panel id="selected-wrap-server-annotator-panel">
            <teian:annotator-panel-element id="teian-search-string-label">Enter search string </teian:annotator-panel-element>
            <teian:annotator-panel-element id="teian-search-button">Search</teian:annotator-panel-element>
            <teian:annotator-panel-element id="teian-search-list-label">List of search results</teian:annotator-panel-element>
        </teian:common-panel>
