    <teian:annotator id="date-annotator">
        <teian:toolbar-button-title>Date</teian:toolbar-button-title>
        <teian:toolbar-button-label>date</teian:toolbar-button-label>
        <teian:annotator-panel>
            <teian:annotator-panel-title>Enter date in format YYYY-MM-DD</teian:annotator-panel-title>
            <teian:annotator-panel-element id="date-when-label">Enter date: </teian:annotator-panel-element>
        </teian:annotator-panel>
    </teian:annotator>
    <teian:annotator id="ref-annotator">
        <teian:toolbar-button-title>Hyperlink</teian:toolbar-button-title>
        <teian:toolbar-button-label>ref</teian:toolbar-button-label>
        <teian:annotator-panel>
            <teian:annotator-panel-title>Enter hyperlink target</teian:annotator-panel-title>
            <teian:annotator-panel-element id="ref-target-label">Enter hyperlink target: </teian:annotator-panel-element>
        </teian:annotator-panel>
    </teian:annotator>
    <teian:annotator id="persName-annotator">
        <teian:toolbar-button-title>Person</teian:toolbar-button-title>
        <teian:toolbar-button-label>persName</teian:toolbar-button-label>
        <teian:annotator-panel>
            <teian:annotator-panel-title>Person</teian:annotator-panel-title>
        </teian:annotator-panel>
    </teian:annotator>
    <teian:annotator id="orgName-annotator">
        <teian:toolbar-button-title>Organization</teian:toolbar-button-title>
        <teian:toolbar-button-label>orgName</teian:toolbar-button-label>
        <teian:annotator-panel>
            <teian:annotator-panel-title>Organization</teian:annotator-panel-title>
        </teian:annotator-panel>
    </teian:annotator>
    <teian:annotator id="lb-annotator">
        <teian:toolbar-button-title>Insert a line break</teian:toolbar-button-title>
        <teian:toolbar-button-label>lb</teian:toolbar-button-label>
    </teian:annotator>
    <teian:annotator id="pb-annotator">
        <teian:toolbar-button-title>Insert a page break</teian:toolbar-button-title>
        <teian:toolbar-button-label>pb</teian:toolbar-button-label>
    </teian:annotator>
    <teian:annotator id="teiannParagraphBtn">
        <teian:toolbar-button-title>Insert a paragraph.</teian:toolbar-button-title>
        <teian:toolbar-button-label/>
    </teian:annotator>
    <teian:annotator id="gloss-annotator">
        <teian:toolbar-button-title>Glossary Term</teian:toolbar-button-title>
        <teian:toolbar-button-label>gloss</teian:toolbar-button-label>
        <teian:annotator-panel>
            <teian:annotator-panel-title>Glossary term</teian:annotator-panel-title>
        </teian:annotator-panel>
    </teian:annotator>
    <teian:annotator id="placeName-annotator">
        <teian:toolbar-button-title>Geo-Location</teian:toolbar-button-title>
        <teian:toolbar-button-label>placeName</teian:toolbar-button-label>
        <teian:annotator-panel>
            <teian:annotator-panel-title>Place Name</teian:annotator-panel-title>
        </teian:annotator-panel>
    </teian:annotator>
    <teian:annotator id="strong-annotator">
        <teian:toolbar-button-title>Insert strong style</teian:toolbar-button-title>
        <teian:toolbar-button-label>
            <b>B</b>
        </teian:toolbar-button-label>
    </teian:annotator>
    <teian:annotator id="italic-annotator">
        <teian:toolbar-button-title>Insert italic style</teian:toolbar-button-title>
        <teian:toolbar-button-label>
            <i>I</i>
        </teian:toolbar-button-label>
    </teian:annotator>
    <teian:annotator id="underline-annotator">
        <teian:toolbar-button-title>Insert underline style</teian:toolbar-button-title>
        <teian:toolbar-button-label>
            <u>U</u>
        </teian:toolbar-button-label>
    </teian:annotator>
    <teian:annotator id="strikethrough-annotator">
        <teian:toolbar-button-title>Insert strikethrough style</teian:toolbar-button-title>
        <teian:toolbar-button-label>
            <s>S</s>
        </teian:toolbar-button-label>
    </teian:annotator>
    <teian:annotator id="label-annotator">
        <teian:toolbar-button-title>Insert label</teian:toolbar-button-title>
        <teian:toolbar-button-label>label</teian:toolbar-button-label>
    </teian:annotator>
    <teian:annotator id="list-annotator">
        <teian:toolbar-button-title>Insert a list</teian:toolbar-button-title>
        <teian:toolbar-button-label>list</teian:toolbar-button-label>
    </teian:annotator>
    <teian:annotator id="listBibl-annotator">
        <teian:toolbar-button-title>Insert a listBibl</teian:toolbar-button-title>
        <teian:toolbar-button-label>listBibl</teian:toolbar-button-label>
    </teian:annotator>
    <teian:annotator id="teiannPageBreakBtn">
        <teian:toolbar-button-title>Insert a page break with three attributes.</teian:toolbar-button-title>
        <teian:toolbar-button-label/>
    </teian:annotator>
