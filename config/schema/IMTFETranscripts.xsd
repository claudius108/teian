    <xs:element name="elementSpec">
        <xs:annotation>
            <xs:documentation>(element specification) documents the structure, content, and purpose of a single element type.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:group minOccurs="0" maxOccurs="unbounded" ref="ns1:tei_model.glossLike"/>
                <xs:element minOccurs="0" ref="ns1:classes"/>
                <xs:element minOccurs="0" ref="ns1:content"/>
                <xs:element minOccurs="0" ref="ns1:valList"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="ns1:constraintSpec"/>
