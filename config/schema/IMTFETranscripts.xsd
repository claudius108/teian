                                <xs:documentation>(parameter entity) </xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="dt">
                            <xs:annotation>
                                <xs:documentation>(datatype entity) </xs:documentation>
                            </xs:annotation>
