                <xs:element minOccurs="0" maxOccurs="unbounded" ref="ns1:remarks"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="ns1:listRef"/>
            </xs:sequence>
            <xs:attributeGroup ref="ns1:tei_att.global.attributes"/>
            <xs:attributeGroup ref="ns1:tei_att.identified.attributes"/>
            <xs:attribute name="ns" default="http://www.tei-c.org/ns/1.0" type="ns1:tei_data.namespace">
