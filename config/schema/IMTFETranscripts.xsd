        <xs:complexType>
            <xs:sequence>
                <xs:group minOccurs="0" maxOccurs="unbounded" ref="ns1:tei_model.glossLike"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="ns1:exemplum"/>
                <xs:element minOccurs="0" ref="ns1:remarks"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="ns1:listRef"/>
            </xs:sequence>
            <xs:attributeGroup ref="ns1:tei_att.global.attributes"/>
            <xs:attributeGroup ref="ns1:tei_att.identified.attributes"/>
            <xs:attribute name="type">
                <xs:annotation>
                    <xs:documentation>type of module to be generated</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="schemaSpec">
        <xs:annotation>
            <xs:documentation>(schema specification) generates a TEI-conformant schema and documentation for it.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:group minOccurs="0" maxOccurs="unbounded" ref="ns1:tei_model.glossLike"/>
